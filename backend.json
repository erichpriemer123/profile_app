{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "age": {
        "type": "string"
      },
      "food": {
        "type": "string"
      },
      "gender": {
        "type": "string",
        "enum": ["man", "f"]
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      }
    },
    "required": ["age", "food", "gender", "image", "name"]
  }
}
